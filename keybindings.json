[
    {
        "key": "cmd+shift+m",
        "command": "type",
        "args": {
            "text": " |> "
        },
        "when": "editorLangId == r || terminalType == r || terminalType == r-interactive"
    },
    {
        "key": "cmd+shift+click",
        "command": "r.runCommandWithEditorPath",
        "args": "view_df(${selection})",
        "when": "editorLangId == 'r'"
    },
    {
        "key": "cmd+shift+v",
        "command": "workbench.action.tasks.runTask",
        "args": "View Data Frame",
        "when": "editorLangId == 'r'"
    }
]