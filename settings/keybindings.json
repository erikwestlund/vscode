// Place your key bindings in this file to override the defaults
[
    {
        "key": "cmd+l",
        "command": "kilo-code.focusKiloCodeView"
    },
    {
        "key": "cmd+o",
        "command": "kilo-code.newTask"
    },
    {
        "key": "cmd+r",
        "command": "kilo-code.generateSuggestedEdits"
    },
    {
        "key": "cmd+i",
        "command": "composerMode.agent"
    },
    {
        "key": "cmd+shift+m",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "|> "
        },
        "when": "terminalFocus"
    },
    {
        "key": "cmd+shift+m",
        "command": "type",
        "args": {
            "text": "|> "
        },
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+shift+click",
        "command": "r.runCommandWithEditorPath",
        "args": "view_df(${selection})",
        "when": "editorLangId == 'r'"
    },
    {
        "key": "cmd+shift+v",
        "command": "workbench.action.tasks.runTask",
        "args": "View Data Frame",
        "when": "editorLangId == 'r'"
    },
    {
        "key": "cmd+shift+t",
        "command": "workbench.action.tasks.runTask",
        "args": "Open All Terminals"
    },
    {
        "key": "shift+cmd+s shift+cmd+r",
        "command": "workbench.action.openGlobalKeybindings"
    },
    {
        "key": "cmd+r cmd+s",
        "command": "-workbench.action.openGlobalKeybindings"
    },
    {
        "key": "cmd+t",
        "command": "workbench.action.terminal.focus",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "cmd+t",
        "command": "-workbench.action.terminal.new",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "ctrl+shift+`",
        "command": "-workbench.action.terminal.new",
        "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
    },
    {
        "key": "shift+cmd+n",
        "command": "-workbench.action.newWindow"
    },
    {
        "key": "shift+cmd+n",
        "command": "explorer.newFolder"
    },
    {
        "key": "shift+cmd+r",
        "command": "-workbench.action.reloadWindow",
        "when": "isDevelopment"
    },
    {
        "key": "shift+cmd+r",
        "command": "-rerunSearchEditorSearch",
        "when": "inSearchEditor"
    },
    {
        "key": "shift+cmd+r",
        "command": "-workbench.action.showAllSymbols"
    },
    {
        "key": "shift+cmd+r",
        "command": "quarto.runAllCells",
        "when": "editorTextFocus && !findInputFocussed && !replaceInputFocussed && editorLangId == 'quarto'"
    },
    {
        "key": "alt+cmd+r",
        "command": "-quarto.runAllCells",
        "when": "editorTextFocus && !findInputFocussed && !replaceInputFocussed && editorLangId == 'quarto'"
    },
    {
        "key": "shift+cmd+r",
        "command": "quarto.runAllCells",
        "when": "activeCustomEditorId == 'quarto.visualEditor'"
    },
    {
        "key": "alt+cmd+r",
        "command": "-quarto.runAllCells",
        "when": "activeCustomEditorId == 'quarto.visualEditor'"
    },
    {
        "key": "shift+cmd+k",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+k",
        "command": "-editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+d",
        "command": "-workbench.view.debug",
        "when": "viewContainer.workbench.view.debug.enabled"
    },
    {
        "key": "shift+cmd+d",
        "command": "-composer.fixerrormessage",
        "when": "@composer.isCursorOnLint"
    },
    {
        "key": "shift+cmd+d",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+down",
        "command": "-editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+alt+o",
        "command": "-workbench.action.tasks.runTask",
        "args": "Format with Black"
    },
    {
        "key": "alt+cmd+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+cmd+up",
        "command": "-editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+up",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "alt+cmd+up",
        "command": "-quickInput.previousSeparator",
        "when": "inQuickInput && quickInputType == 'quickPick'"
    },
    {
        "key": "alt+cmd+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+cmd+down",
        "command": "-editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+down",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "alt+cmd+down",
        "command": "-quickInput.nextSeparator",
        "when": "inQuickInput && quickInputType == 'quickPick'"
    },
    {
        "key": "alt+cmd+l",
        "command": "-toggleFindInSelection",
        "when": "editorFocus"
    },
    {
        "key": "alt+cmd+l",
        "command": "-toggleSearchEditorContextLines",
        "when": "inSearchEditor"
    },
    {
        "key": "alt+cmd+l",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "shift+alt+f",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "alt+cmd+k",
        "command": "-keybindings.editor.recordSearchKeys",
        "when": "inKeybindings && inKeybindingsSearch"
    },
    {
        "key": "alt+cmd+k",
        "command": "quarto.previewScript",
        "when": "quartoRenderScriptActive"
    },
    {
        "key": "shift+cmd+k",
        "command": "-quarto.previewScript",
        "when": "quartoRenderScriptActive"
    },
    {
        "key": "alt+cmd+k",
        "command": "quarto.preview",
        "when": "!quartoRenderScriptActive"
    },
    {
        "key": "shift+cmd+k",
        "command": "-quarto.preview",
        "when": "!quartoRenderScriptActive"
    },
    {
        "key": "cmd+r",
        "command": "workbench.action.gotoSymbol",
        "when": "!accessibilityHelpIsShown && !accessibleViewIsShown"
    },
    {
        "key": "cmd+shift+o",
        "command": "workbench.action.gotoSymbol",
        "when": "!accessibilityHelpIsShown && !accessibleViewIsShown"
    },
    {
        "key": "cmd+shift+o",
        "command": "-workbench.action.tasks.runTask",
        "args": "Format File",
        "when": "editorLangId == python || editorLangId == r"
    },
    {
        "key": "cmd+e",
        "command": "workbench.action.terminal.openDetectedLink",
        "when": "terminalFocus && terminalHasBeenCreated"
    },
    {
        "key": "cmd+shift+o",
        "command": "-workbench.action.remote.showMenu"
    },
    {
        "key": "cmd+shift+o",
        "command": "workbench.action.tasks.runTask",
        "args": "Format File",
        "when": "editorLangId == python || editorLangId == r"
    },
    {
        "key": "cmd+shift+m",
        "command": "type",
        "args": {
            "text": " |> "
        },
        "when": "editorLangId == r || terminalType == r || terminalType == r-interactive"
    },
    {
        "key": "cmd+shift+click",
        "command": "r.runCommandWithEditorPath",
        "args": "view_df(${selection})",
        "when": "editorLangId == 'r'"
    },
    {
        "key": "cmd+shift+v",
        "command": "workbench.action.tasks.runTask",
        "args": "View Data Frame",
        "when": "editorLangId == 'r'"
    },
    {
        "key": "cmd+shift+t",
        "command": "workbench.action.tasks.runTask",
        "args": "Open All Terminals"
    },
    {
        "key": "cmd+shift+[",
        "command": "workbench.action.previousEditor",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+shift+]",
        "command": "workbench.action.nextEditor",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+shift+[",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+shift+]",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+shift+k",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+shift+r",
        "command": "workbench.action.terminal.runSelectedText",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+shift+]",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+shift+[",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+shift+g",
        "command": "workbench.action.terminal.newWithProfile",
        "args": {
            "profile": "Framework (R)"
        }
    },
    {
        "key": "cmd+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+shift+l",
        "command": "workbench.action.tasks.runTask",
        "args": "Format and Lint",
        "when": "editorTextFocus && !editorReadonly && editorLangId == 'r'"
    },
    {
        "key": "ctrl+shift+b",
        "command": "workbench.action.tasks.runTask",
        "args": "R: Build Package",
        "when": "editorLangId == r"
    },
    {
        "key": "ctrl+shift+d",
        "command": "workbench.action.tasks.runTask",
        "args": "R: Document Package",
        "when": "editorLangId == r"
    },
    {
        "key": "alt+shift+1",
        "command": "workbench.view.explorer",
        "when": "sideBarVisible"
    },
    {
        "key": "alt+shift+2",
        "command": "workspaceViewer.focus",
        "when": "sideBarVisible"
    },
    {
        "key": "shift+cmd+i",
        "command": "workspaceViewer.focus",
        "when": "sideBarVisible"
    },
    {
        "key": "shift+enter",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "\\\r\n"
        },
        "when": "terminalFocus"
    }
]